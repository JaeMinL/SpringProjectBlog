<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper 
	PUBLIC "-//mabatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org./dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zerock.mapper.PicsMapper">

	<select id="getList" resultType="org.zerock.domain.PicsVO">
	<![CDATA[
	select * from tb_pics
	]]>
	</select>
	
	<insert id="insert">
	insert into tb_pics (taken_dt, fl_nm, gps_la, gps_lo)
	values (#{taken_dt}, #{fl_nm}, #{gps_la, jdbcType=VARCHAR }, #{gps_lo, jdbcType=VARCHAR})
	</insert>
	
	<select id="read" resultType="org.zerock.domain.PicsVO">
		select * from tb_pics where taken_dt = #{taken_dt} <!-- PicsVO = mapper.read(2020-08-18) -->
	</select>
	
</mapper>